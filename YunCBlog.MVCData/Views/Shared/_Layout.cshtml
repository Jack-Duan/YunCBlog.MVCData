<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="renderer" content="webkit">
    <meta name="keywords" content="@ViewBag.KeyWords" />
    <meta name="description" content="@ViewBag.Description" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="google-site-verification" content="pI1uXHnH66mFLKHDUQcl_7cQAc02gzQjBegzVgpiw0I" />
    <title>梦中的Aoede的博客 - 一个单纯的，渴望技术的小小程序员</title>
    <!-- style样式 -->
    @*<link href="~/plugins/editor_md/css/editormd.preview.css" rel="stylesheet">*@
    <script data-ad-client="ca-pub-2252152819722406" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>


    @Styles.Render("~/Content/css")
    @Styles.Render("~/Comment/css")
    <!-- javascript -->
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("Css", false) <!--子页面需要的css将在此插入-->
    @RenderSection("Js", false) <!--子页面需要的js将在此插入-->
    <script>
        var _hmt = _hmt || [];
        (function () {
            var hm = document.createElement("script");
            hm.src = "https://hm.baidu.com/hm.js?4cb6fa028123a1782c834d112da01f40";
            var s = document.getElementsByTagName("script")[0];
            s.parentNode.insertBefore(hm, s);
        })();
    </script>
</head>
<body>
    <!-- header -->
    @{Html.RenderAction("_Header", "Home");}
    <!-- content -->
    @RenderBody()
    <!-- footer -->
    @Html.Partial("_Footer")
    <a href="#" class="cd-top">Top</a>
</body>
</html>
